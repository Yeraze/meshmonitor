    "archiver": "^7.0.1",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.4.1",
    "better-sqlite3-session-store": "^0.1.0",
    "cors": "^2.8.5",
    "cron-validator": "^1.4.0",
    "express": "^5.1.0",
