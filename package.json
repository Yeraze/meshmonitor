    "leaflet": "^1.9.4",
    "morgan": "^1.10.1",
    "node-cron": "^4.2.1",
    "openid-client": "^6.8.1",
    "protobufjs": "^7.5.4",
    "qrcode": "^1.5.4",
    "react": "^19.0.0",
    "react-dom": "^19.2.0",
    "react-leaflet": "^5.0.0",
    "recharts": "^3.3.0",
    "rotating-file-stream": "^3.2.7",
